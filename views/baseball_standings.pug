html
	head
		title= "Trifecta Fantasy Home Page"
		link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css')
		link(rel='stylesheet', href='/css/styles.css')

	body
		header
			navbar
				a(href= '/' class = 'navbar') Home Page
				a(href= '/scrape_standings') Baseball Standings
		

		h2= "2016 Baseball Standings"


		h4= "Head to Head Standings"
		table
			thead
				tr
					th TEAM
					th WINS
					th LOSSES
					th TIES
					th WIN %
					th DIVISION
					th TRIFECTA H2H POINTS
			tbody
				each readin in h2h_standings
					tr
						each val, cat in readin
							if cat === "team"
								td= val
							if cat === "wins"
								td= val
							if cat === "losses"
								td= val
							if cat === "ties"
								td= val
							if cat === "win_per"
								td= val
							if cat === "division"
								td= val
							if cat === "h2h_trifecta_points"
								td= val
													