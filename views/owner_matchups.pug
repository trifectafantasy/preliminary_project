html
	head
		title= year1 + "-" + year2 + " Owner to Owner Matchups"
		include ./JADE-Bootstrap/_bootstrap
		include mixins.pug
		link(rel='stylesheet', type="text/css", href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css')
		link(rel='stylesheet', type="text/css", href='/css/styles.css')

	body
		header
			+headliner

									
		h2= owner + "'s " + year1 + "-" + year2 + " Trifecta Head 2 Head Owner Matchups"

		if baseball_in_season

			h4= "Overall"
			table
				thead
					tr
						th OPPOSING OWNER
						th FOOTBALL WIN %
						th BASKETBALL WIN %
						th BASEBALL WIN %
						th OVERALL WIN%
				tbody
					each readin_tri in trifecta_matchups
						tr
							each val, cat in readin_tri
								if cat === "opposing_owner"
									td= val
								if cat === "football_win_per"
									td= parseFloat(val).toFixed(3)
								if cat === "basketball_win_per"
									td= parseFloat(val).toFixed(3)
								if cat === "baseball_win_per"
									td= parseFloat(val).toFixed(3)
								if cat === "total_win_per"
									td= parseFloat(val).toFixed(3)

			h4= "Football"
			table
				thead
					tr
						th OPPOSING OWNER
						th WINS
						th LOSSES
						th TIES
						th WIN %
						th POINTS FOR
						th POINTS AGAINST
						th POINT DIFFERENTIAL
				tbody
					each readin in football_matchups
						tr
							each val, cat in readin
								if cat === "opposing_owner"
									td= val
								if cat === "wins"
