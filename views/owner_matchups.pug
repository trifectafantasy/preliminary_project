								if cat === "PF"
									td= parseFloat(val).toFixed(1)
								if cat === "PA"
									td= parseFloat(val).toFixed(1)
								if cat === "pt_diff"
									td= parseFloat(val).toFixed(1)

			h4= "Basketball"
			table
				thead
					tr
						th OPPOSING OWNER
						th WINS
						th LOSSES
						th TIES
						th WIN %

				tbody
